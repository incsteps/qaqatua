{
    "openapi": "3.0.0",
    "info": {
        "title": "QA Encription/Decription API",
        "description": "QA enc/dec api",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://localhost:8000",
            "description": "local server"
        },
        {
            "url": "https://qaqatua.com",
            "description": "production server"
        }
    ],
    "paths": {
        "/api/encrypt": {
            "post": {
                "summary": "Encrypt fields of a json payload",
                "operationId": "82e0cf0919110dcdd53c7ea05981df5e",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "payload": {
                                        "type": "object"
                                    }
                                },
                                "type": "object",
                                "example": {
                                    "payload": {
                                        "msg": "Jessica Smith"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
        },
        "/api/decrypt": {
            "post": {
                "summary": "Decrypt fields of a json payload",
                "operationId": "7fd39b5d5b8d3ed7c2de9c9a213f11ea",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "payload": {
                                        "type": "object"
                                    }
                                },
                                "type": "object",
                                "example": {
                                    "payload": {
                                        "msg": ""
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
        }
    },
    "components": {
        "securitySchemes": {
            "bearerAuth": {
                "type": "http",
                "name": "Authorization",
                "in": "header",
                "scheme": "bearer"
            }
        }
    }
}